        "required": [
          "did"
        ],
        "properties": {
          "did": {
            "type": "string",
            "description": "The DID.",
            "format": "at-identifier"
          }
        }
      },
      "output": {
        "encoding": "application/json",
        "schema": {
          "type": "object",
          "required": [
            "handle"
          ],
          "properties": {
            "handle": {
              "type": "string",
              "description": "The handle.",
              "format": "handle"
            }
          }
        }
      }
    }
  }
}