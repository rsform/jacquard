{
  "lexicon": 1,
  "id": "lol.skittr.server.getMeta",
  "defs": {
    "main": {
      "type": "query",
      "description": "Get information about the AppView",
      "output": {
        "encoding": "application/json",
        "schema": {
          "type": "object",
          "required": [
            "software",
            "version",
            "supported_lexicons"
          ],
          "properties": {
            "software": {
              "type": "string"
            },
            "supported_extensions": {
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            "supported_lexicons": {
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            "version": {
              "type": "string"
            }
          }
        }
      }
    }
  }
}