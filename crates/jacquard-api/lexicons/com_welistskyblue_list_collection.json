{
  "lexicon": 1,
  "id": "com.welistskyblue.list.collection",
  "description": "A record that holds a collection of active task AT-URIs for a user.",
  "defs": {
    "main": {
      "type": "record",
      "key": "self",
      "record": {
        "type": "object",
        "required": [
          "uris",
          "createdAt"
        ],
        "properties": {
          "createdAt": {
            "type": "string",
            "format": "datetime"
          },
          "updatedAt": {
            "type": "string",
            "format": "datetime"
          },
          "uris": {
            "type": "array",
            "description": "List of AT-URIs controlled by the user.",
            "items": {
              "type": "string",
              "format": "at-uri"
            }
          }
        }
      }
    }
  }
}