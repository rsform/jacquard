{
  "lexicon": 1,
  "id": "place.stream.chat.defs",
  "defs": {
    "messageView": {
      "type": "object",
      "required": [
        "uri",
        "cid",
        "author",
        "record",
        "indexedAt"
      ],
      "properties": {
        "author": {
          "type": "ref",
          "ref": "app.bsky.actor.defs#profileViewBasic"
        },
        "chatProfile": {
          "type": "ref",
          "ref": "place.stream.chat.profile"
        },
        "cid": {
          "type": "string",
          "format": "cid"
        },
        "deleted": {
          "type": "boolean",
          "description": "If true, this message has been deleted or labeled and should be cleared from the cache"
        },
        "indexedAt": {
          "type": "string",
          "format": "datetime"
        },
        "record": {
          "type": "unknown"
        },
        "replyTo": {
          "type": "union",
          "refs": [
            "place.stream.chat.defs#messageView"
          ]
        },
        "uri": {
          "type": "string",
          "format": "at-uri"
        }
      }
    }
  }
}