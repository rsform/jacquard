{
  "lexicon": 1,
  "id": "us.polhem.blog.defs",
  "description": "",
  "defs": {
    "aspectRatio": {
      "type": "object",
      "description": "width:height represents an aspect ratio. It may be approximate, and may not correspond to absolute dimensions in any given unit.",
      "required": [
        "width",
        "height"
      ],
      "properties": {
        "height": {
          "type": "integer",
          "minimum": 1
        },
        "width": {
          "type": "integer",
          "minimum": 1
        }
      }
    }
  }
}