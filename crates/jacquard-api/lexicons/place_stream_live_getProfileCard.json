{
  "lexicon": 1,
  "id": "place.stream.live.getProfileCard",
  "defs": {
    "main": {
      "type": "query",
      "description": "Get an OG image associated with a given account.",
      "parameters": {
        "type": "params",
        "required": [
          "id"
        ],
        "properties": {
          "id": {
            "type": "string",
            "description": "The DID or handle of the account."
          }
        }
      },
      "output": {
        "encoding": "*/*"
      },
      "errors": [
        {
          "name": "RepoNotFound"
        }
      ]
    }
  }
}