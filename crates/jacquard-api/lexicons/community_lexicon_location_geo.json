  "id": "community.lexicon.location.geo",
  "defs": {
    "main": {
      "type": "object",
      "description": "A physical location in the form of a WGS84 coordinate.",
      "required": [
        "latitude",
        "longitude"
      ],
      "properties": {
        "altitude": {
          "type": "string"
        },
        "latitude": {
          "type": "string"
        },
        "longitude": {
          "type": "string"
        },
        "name": {
          "type": "string",
          "description": "The name of the location."
        }
      }
    }
  }
}